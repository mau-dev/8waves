<div class="container landing-container d-flex ">
		<div class="float-left  reg-left col-md-6">
				<img class="landing-img" src="<%= image_url 'landing-grid.png' %>" />
		</div>
		<div class="form-group reg-form  rounded mt-2 col-md-6 float-right">
				<div class="inner-form col-md-10 mt-1 mx-4 ">
						<img width="100%" class="pb-4" src="<%= image_url 'waves-registration.png' %>" />
						<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
						<div class="field input">
								<%#= <%= f.label :email %>
								<%= f.email_field :email, autofocus: true, autocomplete: "email",  class: "col-md-12", placeholder: "Email" %>
						</div>
						<div class="field input ">
								<%= f.password_field :password, autocomplete: "current-password", class: "col-md-12", placeholder: "Your password" %>
						</div>


						<% if devise_mapping.rememberable? %>
						<div class="field input-checkbox">
								<%= f.check_box :remember_me %>
								<%= f.label :remember_me %>
						</div>
						<div class="actions d-block mb-3 text-center">
								<%= f.submit "Log in", class: "signup-btn" %>
						</div>
						<div class="actions d-block login-btn mb-3 text-center">
								<a class="landing-btn nested-a" href="/">Back</a>
						</div>
						<% end %>
						<%#	<%= render "devise/shared/links" %>
						<%# <%= render "devise/shared/links" %>
				</div>
				<% end %>
		</div>
</div>